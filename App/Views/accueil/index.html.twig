{% block javascripts %}
    <script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
    <script>
        function init() {
            var grid = $('.grid').masonry({
                percentPosition: true,
                columnWidth: '.grid-sizer',
                itemSelector: '.grid-item',
                gutter: 10,
                stagger: 30,
            });

            {% for img in images %}
            let item{{ loop.index }} = document.createElement('div');
            $(item{{ loop.index }}).addClass("grid-item");

            let img{{ loop.index }} = document.createElement('img');
            $(img{{ loop.index }}).addClass('grid-image');
            $(img{{ loop.index }}).attr('src', 'public/images/{{ img }}');
            $(img{{ loop.index }}).attr('srcset', 'public/thumbnails500/{{ img }} 500w, public/thumbnails300/{{ img }} 300w');

            $(item{{ loop.index }}).append(img{{ loop.index }});
            delete img{{ loop.index }};

            let overlay{{ loop.index }} = document.createElement('div');
            $(overlay{{ loop.index }}).addClass('overlay d-flex justify-content-center align-items-center');

            let btn_show{{ loop.index }} = document.createElement('a');
            $(btn_show{{ loop.index }}).html('Détails');
            $(btn_show{{ loop.index }}).addClass('btn btn-outline-light rounded-0 mr-1');
            $(btn_show{{ loop.index }}).attr('href', '/devoir-idc2019/image/{{ img }}');

            let btn_edit{{ loop.index }} = document.createElement('a');
            $(btn_edit{{ loop.index }}).html('Modifier');
            $(btn_edit{{ loop.index }}).addClass('btn btn-outline-light rounded-0');
            $(btn_edit{{ loop.index }}).attr('href', '/devoir-idc2019/image/{{ img }}/edit');

            let btn_delete{{ loop.index }} = document.createElement('a');
            $(btn_delete{{ loop.index }}).html('Supprimer');
            $(btn_delete{{ loop.index }}).addClass('btn btn-danger rounded-0 ml-1');
            $(btn_delete{{ loop.index }}).attr('href', '/devoir-idc2019/image/{{ img }}/delete');
            $(btn_delete{{ loop.index }}).on('click', function () {
                return confirm('Êtes-vous sûr ?');
            });

            $(overlay{{ loop.index }}).append(btn_show{{ loop.index }});
            $(overlay{{ loop.index }}).append(btn_edit{{ loop.index }});
            $(overlay{{ loop.index }}).append(btn_delete{{ loop.index }});

            $(item{{ loop.index }}).append(overlay{{ loop.index }});
            delete overlay{{ loop.index }};
            delete btn_show{{ loop.index }};
            delete btn_edit{{ loop.index }};
            delete btn_delete{{ loop.index }};

            setTimeout(function () {
                grid.append(item{{ loop.index }} ).masonry('appended', item{{ loop.index }} );
                delete item{{ loop.index }};
                grid.masonry();
            }, 300 + 50 *{{ loop.index }});

            $(item{{ loop.index }}).on('click', function (e) {
                if ($(e.target).attr('href') !== undefined && $(e.target).attr('href') !== null) {
                    performPageTransition($(e.target).attr('href'));
                }
                return false;
            });
            {% endfor %}
        }

        $(document).ready(function () {
            init();
        });
    </script>

{% endblock %}
